{
    "name": "passchn/cakephp-assets",
    "description": "Asset management plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^3.10.0",
        "cakephp/migrations": "^2.0.0",
        "josegonzalez/cakephp-upload": "^4.0",
        "league/csv": "^9.8",
        "nette/finder": "^2.5",
        "nette/utils": "^3.2",
        "intervention/image": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0 || ^8.5"
    },
    "autoload": {
        "psr-4": {
            "Assets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Assets\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
